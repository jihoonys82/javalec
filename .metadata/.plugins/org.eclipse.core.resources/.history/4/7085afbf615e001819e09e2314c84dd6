package operator;

public class BinaryEx_06 {
		public static void main(String[] args) {
			//Bitwise logical operator
			
			// &(and) |(or) ^(xor)
			
			int num1 = 21;  //...0001 0101
			int num2 = 24;  //...0001 1000
			
			System.out.println(num1 & num2); // 0001 0000
			System.out.println(num1 | num2); // 0001 1101
			System.out.println(num1 ^ num2); // 0000 1111
			
		}
}
