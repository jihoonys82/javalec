package array;

import java.util.Arrays;

public class ArrayEtc_01 {
	public static void main(String[] args) {
		
		int[] arr = {4, 9, 2, 1, 3};
		
		System.out.println("======before sorting======");
		for(int i=0;i<arr.length;i++) {
			System.out.print(arr[i] + " ");
		}
		System.out.println();
		
		//Sorting via API
		System.out.println("======After sorting======");
		Arrays.sort(arr);
		for(int i=0;i<arr.length;i++) {
			System.out.print(arr[i] + " ");
		}
		System.out.println();
		
	}
}
