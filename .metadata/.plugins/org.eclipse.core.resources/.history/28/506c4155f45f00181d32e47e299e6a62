package condition;

public class SwitchEx_03 {

	public static void main(String[] args) {
		
		char ch = 'q';
		switch(ch) { 
		case 'Q':
			System.out.println("uppercase Q");
			break;
		case 'q':
			System.out.println("lowcase q");
			break;
		case 'a':
			System.out.println("a");
			break;
		case 'B':
			System.out.println("B");
			break;		
		}
	}

}
