package array;

public class ArrayEtc_02 {
	public static void main(String[] args) {
		
		int arr[] = {10, 20, 30};
		System.out.println(arr); // Reference 
		
		String str = "Apple";
		System.out.println(str); // string
		
		// String is reference type but it is not return reference address. 
		// String type variable return strings for usability. 
	}
}
