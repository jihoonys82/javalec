package condition.loop;

public class EtcEx_01 {

	public static void main(String[] args) {
		
		// Labeled break
		// 
		
		
		for(int i=0;i<10;i++) {
			System.out.println(i);
			
			if(i==5) {
				break outer; 
			}
			
		}
		
		System.out.println("?");
		outer :
		System.out.println("!!");
		
	}

}
