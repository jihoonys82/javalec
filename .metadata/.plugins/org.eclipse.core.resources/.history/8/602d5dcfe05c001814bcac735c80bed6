package operator;

public class BinaryEx_04 {
	public static void main(String[] args) {
		//Logical operator 
		// logical value(true / false)의 논리식 결과를 반환하는 연산자
		// 논리값의 관계를 따지는 연산
		
		// AND(&&) , OR(||) , XOR(없음), NOT(!) 
		
		boolean b1 = true;
		boolean b2 = false; 
		
		System.out.println(b1 && b2);
	}
}
