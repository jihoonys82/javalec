package operator;

public class BinaryEx_02 {
	public static void main(String[] args) {
		// relation operator (관계연산자 or 비교연산자) 
		// 이항연산자
		//  왼쪽항을 기준으로 오른쪽 항을 비교하여 true/false로 관계에 대한 비교 결과를 반환(리턴)한다.
		
		int num1 = 11;
		int num2 = 22;
		
		System.out.println( num1 == num2 );
	}
}
