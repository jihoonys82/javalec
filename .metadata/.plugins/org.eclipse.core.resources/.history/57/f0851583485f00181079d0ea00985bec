package condition.star;

public class Star_13 {
	public static void main(String[] args) {
		for(int i=0;i<10;i++) {
			if(i<5) {
				for(int j=5;j>i;j--) {
					System.out.print("*");
				}				
			} else { 
				for(int j=5;j>i-4;j--) {
					System.out.print("*");
				}
			}
			System.out.println();
		}
	}
}
