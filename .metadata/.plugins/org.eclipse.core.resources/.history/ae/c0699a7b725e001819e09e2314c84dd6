package condition;

public class IfEx_01 {
	public static void main(String[] args) {
		
		// if statement 
	
		// if(condition) 
		// executed when the condition is true(boolean).
		
		if(true) 
			System.out.println("วัมู");
		
		if(true) {
			System.out.println("test");
			System.out.println("is it printed?");
		}
			
		
	}
}
