package condition;

public class IfElseEx_02 {
	public static void main(String[] args) {
		//if(condition) { 
		//	execute when the condition is true.
		//} else {
		//	execute when the condition is false. 
		//}
		boolean cond = false;
		
		if(cond) { 
			System.out.println("true"); 
		} else { 
			System.out.println("false");
		}
		
	}
}
